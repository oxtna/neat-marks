{
  "name": "Neatmarks",
  "manifest_version": 3,
  "version": "1.0.0",
  "author": "oxtna",
  "description": "chrome bookmarks but better",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "icons": {
    "16": "logo192.png",
    "24": "logo192.png",
    "32": "logo192.png",
    "64": "logo192.png",
    "192": "logo192.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Neatmarks"
  }
}